  </configSections>
  <appSettings>
    <!-- update these with your BotId, Microsoft App Id and your Microsoft App Password-->
    <add key="BotId" value="YourBotId" />
    <add key="MicrosoftAppId" value="5ccc616f-0b49-48b5-a818-51d2caef59c5" />
    <add key="SecretUri" value="https://asksapbotkeys.vault.azure.net" />
